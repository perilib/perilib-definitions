    "protocols": [
        {
            "name": "Bluetooth Low Energy (BLE1xx series)",
            "type": "stream",
            "format": "binary",
            "token": "ble",
            "header": [
                { "name": "message_type", "bits": "1" },
                { "name": "technology_type", "bits": "4" },
                { "name": "payload_length", "bits": "11" },
                { "name": "group_id", "bits": "8" },
                { "name": "method_id", "bits": "8" }
            ],
            "test_packet_start": "$packet.header.technology_type == 0",
            "test_packet_complete": "$packet.length == $packet.header.payload_length + 4",
            "packets": {
                "commands": {
                    "direction": "both",
                    "token_out": "cmd",
                    "token_in": "rsp"
                },
                "events": {
                    "direction": "in",
                    "token_in": "evt"
                }
            }
        }
    ]
}
